{
  "name": "common-vue-ui",
  "author": "kybetter",
  "version": "0.0.11",
  "main": "lib/cui.min.js",
  "style": "lib/styles/index.css",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "yarn build:prod && yarn build:components && yarn build:style",
    "build:prod": "webpack --config build/webpack.prod.config.js",
    "build:components": "webpack --config build/webpack.components.config.js",
    "build:style": "sass --no-source-map src/styles:lib/styles",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "watch:style": "sass --no-source-map --watch src/styles:lib/styles"
  },
  "peerdependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.1.0",
    "html-webpack-plugin": "^5.3.1",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "sass": "^1.30.0",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.8.2",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "dependencies": {}
}
